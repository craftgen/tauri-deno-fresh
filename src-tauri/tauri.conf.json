{
  "productName": "tauri-deno-fresh",
  "version": "0.1.0",
  "identifier": "com.tauri-deno-fresh.app",
  "build": {
    "beforeDevCommand": "deno run start",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "deno run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "tauri-deno-fresh",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "externalBin": ["binaries/deno"]
  }
}
